{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "replay_path_validation",
  "type": "object",
  "properties": {
    "Program": {
      "type": "string",
      "pattern": "^(.*)$",
      "minLength": 3,
      "maxLength": 150
    },
    "Event": {
      "type": "string",
      "pattern": "^(.*)$",
      "minLength": 3,
      "maxLength": 150
    },
    "ContentGroup": {
      "type": "string",
      "minLength": 3,
      "maxLength": 50,
      "pattern": "^(.*)$"
    },
    "TransitionName": {
      "type": "string",
      "minLength": 3,
      "maxLength": 150,
      "pattern": "^(.*)$"
    },
    "ReplayId": {
      "type": "string",
      "minLength": 3,
      "maxLength": 50,
      "pattern": "^(.*)$"
    },
    "AudioTrack": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50,
      "pattern": "^(.*)$"
    }
  }
}
