    "Event": {
      "type": "string",
      "pattern": "^(.*)$",
      "minLength": 3,
      "maxLength": 50
    },
    "Plugin": {
      "type": "string",
      "pattern": "^(.*)$",
      "minLength": 3,
      "maxLength": 50
    },
    "Start": {
      "type": "string",
      "pattern": "^(.*)$",
      "minLength": 3,
      "maxLength": 50
    },
    "End": {
      "type": "string",
      "pattern": "^(.*)$",
      "minLength": 3,
      "maxLength": 50
    },
    "StartFrom": {
      "type": "number",
      "pattern": "^(.*)$",
